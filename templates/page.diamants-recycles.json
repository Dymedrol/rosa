    "d8037b2c-93f5-4ed8-8ab3-69c24ce46c15": {
      "type": "image-banner",
      "settings": {
        "tag": "h1",
        "is_full_screen": false,
        "alignment": "center",
        "text_color": "#ffffff",
        "image_banner_title_text": "<p>LES DIAMANTS RECYCLÉS PAR ROSA MAÏTEA<\/p>",
        "is_title_big": false,
        "image_banner_subtitle_text": "",
        "is_text_big": false,
        "image_banner_link_text": "",
        "image_banner_link_url": "",
